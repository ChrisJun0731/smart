package com.sumridge.smart.bean;

import java.util.List;
import java.util.Map;

/**
 * Created by liu on 16/4/26.
 */
public class CommonAggBean {
    private String id;
    private int count;
    private List<Map<String,String>> detail;

    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    public int getSize(){
        return detail==null?0:detail.size();
    }
    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public List<Map<String, String>> getDetail() {
        return detail;
    }

    public void setDetail(List<Map<String, String>> detail) {
        this.detail = detail;
    }
}
