<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        <small>Match List</small>
    </h1>

</section>

<!-- Main content -->
<section class="content">

    <div class="row">
        <div class="col-md-9">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <small class="box-title" style="font-size: 80%">

                    </small>

                    <div class="box-tools pull-right">
                        <div class="btn-group">
                            <input type="text" class="form-control input-sm" placeholder="Search Match">
                            <span class="glyphicon glyphicon-search form-control-feedback"></span>
                        </div>
                        <div class="btn-group" uib-dropdown>
                            <button id="single-button" type="button" class="btn btn-success"  uib-dropdown-toggle><i class="fa fa-plus"></i></button>
                            <ul class="dropdown-menu" uib-dropdown-menu role="menu" aria-labelledby="single-button">
                                <li role="menuitem"><a href="" ng-click="openSaleModal({},'lg')">Add Match</a></li>
                                <li class="divider"></li>
                                <li role="menuitem"><a href="" ng-click="openUploadModal('lg')">Import From File</a></li>
                                <li class="divider"></li>
                                <li role="menuitem"><a href="/common/download?filename=trade-feed.csv" >Download Match Template</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /.box-tools -->
                </div>
                <!-- /.box-header -->
                <div class="box-body no-padding">
                    <div class="mailbox-controls">
                        <!-- Check all button -->
                        <button type="button" class="btn btn-default btn-sm checkbox-toggle" ng-click="allSalesSelect($event)" ng-model="allSelected">
                            <i class="fa fa-square-o"></i>
                        </button>
                        <div class="btn-group">
                            <a ng-click="downloadSelInfo($event)" download="download.csv">
                                <button type="button" class="btn btn-default btn-sm"><i class="fa fa-download"></i></button>
                            </a>
                            <button type="button" class="btn btn-default btn-sm"><i class="fa fa-share"></i></button>
                        </div>
                        <!-- /.btn-group -->
                        <button type="button" class="btn btn-default btn-sm" ng-click="refresh(page)"><i class="fa fa-refresh"></i></button>
                        <div class="pull-right">
                            {{start}}-{{end}}/{{size}}
                            <div class="btn-group">
                                <button type="button" class="btn btn-default btn-sm" ng-click="prePage()"><i class="fa fa-chevron-left"></i></button>
                                <button type="button" class="btn btn-default btn-sm" ng-click="nextPage()"><i class="fa fa-chevron-right"></i></button>
                            </div>
                            <!-- /.btn-group -->
                        </div>
                        <!-- /.pull-right -->
                    </div>
                    <div class="box-body">

                        <table class="table">
                            <tbody><tr>
                                <th style="width: 10px">#</th>
                                <th style="width: 10px"></th>
                                <!--<th>Customer</th>-->
                                <th>Title</th>
                                <th>Status</th>
                                <th>Match Time</th>
                                <!--<th style="width: 40px">Status</th>-->
                            </tr>

                            <tr ng-repeat="sale in saleList">
                                <td><input type="checkbox" ng-checked="sale.checked"/></td>
                                <td class="mailbox-star"><a href="#"><i class="fa fa-star-o text-yellow"></i></a></td>
                                <!--<td><span class="fa fa-fw fa-user text-green"></span>{{ sale.customer }}</td>-->
                                <td ng-bind="sale.title">
                                </td>
                                <td ng-bind="sale.status">
                                </td>
                                <td>
                                    {{sale.createDate | amDateFormat:'YYYY-MM-DD HH:mm:ss'}}
                                </td>
                                <!--<td><span class="badge bg-green">finished</span></td>-->
                            </tr>
                            </tbody></table>

                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer no-padding">

                    </div>
                </div>
                <!-- /. box -->
            </div>

        </div>
        <!-- /.col -->
        <div class="col-md-3">

            <div class="box box-solid collapsed-box">
                <div class="box-header with-border">
                    <h3 class="box-title">Filter</h3>

                    <div class="box-tools">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse" ng-click="collapse($event);isFilterCollapsed=!isFilterCollapsed">
                            <i class="fa fa-plus"></i>
                        </button>
                    </div>
                </div>
                <div uib-collapse="isFilterCollapsed">
                    <ul class="nav nav-pills nav-stacked">
                        <li><a href="#"><input type="checkbox" value="" name=""> recent view<span class="label label-warning pull-right">100+</span></a></li>
                        <li><a href="#"><input type="checkbox" value="" name=""> shared <span class="label label-warning pull-right">65</span></a>
                        </li>
                        <li><a href="#"><input type="checkbox" value="" name=""> star<span class="label label-warning pull-right">35</span></a></li>
                        <li>
                            <a href="#"><i class="fa fa-circle-o"></i> Status<i class="fa fa-angle-left pull-right"></i></a>
                        </li>
                    </ul>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /. box -->
            <div class="box box-solid collapsed-box">
                <div class="box-header with-border">
                    <h3 class="box-title">Sort</h3>

                    <div class="box-tools">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse" ng-click="collapse($event);isSortCollapsed=!isSortCollapsed">
                            <i class="fa fa-plus"></i>
                        </button>
                    </div>
                </div>
                <div uib-collapse="isSortCollapsed">
                    <ul class="nav nav-pills nav-stacked">
                        <li><a ng-click="getSortedList($event,'secId')"><i class="fa fa-sort-up text-light-blue"></i> Info</a></li>
                        <li><a ng-click="getSortedList($event,'settleDate')"><i class="fa fa-sort-up text-light-blue"></i> TradeTime</a></li>
                        <li><a ng-click="getSortedList($event,'quantity')"><i class="fa fa-sort-up text-light-blue"></i> Qty</a></li>
                        <li><a ng-click="getSortedList($event,'netPrice')"><i class="fa fa-sort-up text-light-blue"></i> Price</a></li>
                    </ul>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->

        </div>

    </div>
    <!-- /.row -->

</section>
<!-- /.content -->

<!--Modal -->
<script type="text/ng-template" id="saleCreator.html">
    <div class="modal-header">
        <h3 class="modal-title">Edit Match</h3>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <div class="box-body">
                <div class="form-group">
                    <label class="col-sm-4 control-label">Customer</label>

                    <div class="col-sm-8">
                        <tags-input ng-model="info.customer" display-property="email" key-property="id" placeholder="customer/group" min-length="1" min-tags="1" max-tags="10" add-from-autocomplete-only="true">
                            <auto-complete source="loadNames($query)" min-length="0"></auto-complete>
                        </tags-input>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-4 control-label">Title</label>

                    <div class="col-sm-8">
                        <input type="text" class="form-control" placeholder="title" ng-model="info.title">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-4 control-label">Content</label>

                    <div class="col-sm-8">
                        <input type="text" class="form-control" placeholder="content" ng-model="info.content">
                    </div>
                </div>

                <hr>

            </div>
        </form>
        <div class="form-group">
            <table class="table table-bordered table-hover table-condensed">
                <tr style="font-weight: bold">
                    <td style="width:20%">Cusip</td>
                    <td style="width:10%">Side</td>
                    <td style="width:20%">Quantity</td>
                    <td style="width:20%">Price</td>
                    <td style="width:30%">Edit</td>
                </tr>
                <tr ng-repeat="map in info.salesMap">
                    <td>
                        <!-- editable username (text with validation) -->
                                <span editable-text="map.cusip" e-name="cusip" e-form="rowform" onbeforesave="checkName($data, map.cusip)" e-required>
                                  {{ map.cusip || 'empty' }}
                                </span>
                    </td>
                    <td>
                        <!-- editable status (select-local) -->
                                <span editable-select="map.side" e-name="side" e-form="rowform" e-ng-options="s.value as s.text for s in sides">
                                  {{ showSides(map) }}
                                </span>
                    </td>
                    <td>
                        <!-- editable username (text with validation) -->
                                <span editable-text="map.quantity" e-name="quantity" e-form="rowform" onbeforesave="checkName($data, map.cusip)" e-required>
                                  {{ map.quantity || 'empty' }}
                                </span>
                    </td>
                    <td>
                        <!-- editable username (text with validation) -->
                                <span editable-text="map.price" e-name="price" e-form="rowform" onbeforesave="checkName($data, map.cusip)" e-required>
                                  {{ map.price || 'empty' }}
                                </span>
                    </td>
                    <td style="white-space: nowrap">
                        <!-- form -->
                        <form editable-form name="rowform" onbeforesave="saveMap($data, map.cusip)" ng-show="rowform.$visible" class="form-buttons form-inline" shown="inserted == map">
                            <button type="submit" ng-disabled="rowform.$waiting" class="btn btn-primary">
                                save
                            </button>
                            <button type="button" ng-disabled="rowform.$waiting" ng-click="rowform.$cancel()" class="btn btn-default">
                                cancel
                            </button>
                        </form>
                        <div class="buttons" ng-show="!rowform.$visible">
                            <button class="btn btn-primary" ng-click="rowform.$show()">edit</button>
                            <button class="btn btn-danger" ng-click="removeMap($index)">del</button>
                        </div>
                    </td>
                </tr>
            </table>

            <button class="btn btn-default" ng-click="addMap()">Add Match</button>
        </div>

    </div>
    <div class="modal-footer">
        <button class="btn btn-primary" type="button" ng-click="ok()">Send</button>
        <button class="btn btn-success" type="button" ng-click="cancel()">Smart Match</button>
        <button class="btn btn-warning" type="button" ng-click="cancel()">Cancel</button>
    </div>
</script>

<!--File upload Modal -->
<script type="text/ng-template" id="fileUpload.html">
    <div class="modal-header">
        <h3 class="modal-title">File Upload</h3>
    </div>
    <div class="modal-body">
        <div class="row">

            <div class="col-md-3">

                <h4>Select files</h4>

                <!-- Example: nv-file-select="" uploader="{Object}" options="{Object}" filters="{String}" -->
                <!--Multiple-->
                <input type="file" nv-file-select="" uploader="uploader" multiple  /><br/>

                <!--Single-->
                <!--<input type="file" nv-file-select="" uploader="uploader" />-->
                <!--<h4>Status</h4>-->
                <!--<div class="btn-group">-->
                    <!--<label class="btn btn-primary" ng-model="radioModel" uib-btn-radio="'public'">Public</label>-->
                    <!--<label class="btn btn-primary" ng-model="radioModel" uib-btn-radio="'private'">Private</label>-->
                <!--</div>-->
            </div>

            <div class="col-md-9" style="margin-bottom: 40px">

                <h3>Upload queue</h3>
                <p>Queue length: {{ uploader.queue.length }}</p>

                <table class="table">
                    <thead>
                    <tr>
                        <th width="50%">Name</th>
                        <th ng-show="uploader.isHTML5">Size</th>
                        <th ng-show="uploader.isHTML5">Progress</th>
                        <th>Status</th>
                        <th>Actions</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="item in uploader.queue">
                        <td><strong>{{ item.file.name }}</strong></td>
                        <td ng-show="uploader.isHTML5" nowrap>{{ item.file.size/1024/1024|number:2 }} MB</td>
                        <td ng-show="uploader.isHTML5">
                            <div class="progress" style="margin-bottom: 0;">
                                <div class="progress-bar" role="progressbar" ng-style="{ 'width': item.progress + '%' }"></div>
                            </div>
                        </td>
                        <td class="text-center">
                            <span ng-show="item.isSuccess"><i class="glyphicon glyphicon-ok"></i></span>
                            <span ng-show="item.isCancel"><i class="glyphicon glyphicon-ban-circle"></i></span>
                            <span ng-show="item.isError"><i class="glyphicon glyphicon-remove"></i></span>
                        </td>
                        <td nowrap>
                            <button type="button" class="btn btn-success btn-xs" ng-click="item.upload()" ng-disabled="item.isReady || item.isUploading || item.isSuccess">
                                <span class="glyphicon glyphicon-upload"></span> Upload
                            </button>
                            <button type="button" class="btn btn-warning btn-xs" ng-click="item.cancel()" ng-disabled="!item.isUploading">
                                <span class="glyphicon glyphicon-ban-circle"></span> Cancel
                            </button>
                            <button type="button" class="btn btn-danger btn-xs" ng-click="item.remove()">
                                <span class="glyphicon glyphicon-trash"></span> Remove
                            </button>
                        </td>
                    </tr>
                    </tbody>
                </table>

                <div>
                    <div>
                        Queue progress:
                        <div class="progress" style="">
                            <div class="progress-bar" role="progressbar" ng-style="{ 'width': uploader.progress + '%' }"></div>
                        </div>
                    </div>
                    <button type="button" class="btn btn-success btn-s" ng-click="uploader.uploadAll()" ng-disabled="!uploader.getNotUploadedItems().length">
                        <span class="glyphicon glyphicon-upload"></span> Upload all
                    </button>
                    <button type="button" class="btn btn-warning btn-s" ng-click="uploader.cancelAll()" ng-disabled="!uploader.isUploading">
                        <span class="glyphicon glyphicon-ban-circle"></span> Cancel all
                    </button>
                    <button type="button" class="btn btn-danger btn-s" ng-click="uploader.clearQueue()" ng-disabled="!uploader.queue.length">
                        <span class="glyphicon glyphicon-trash"></span> Remove all
                    </button>
                </div>

            </div>

        </div>


    </div>
    <div class="modal-footer">
        <button class="btn btn-warning" type="button" ng-click="cancel()">Close</button>
    </div>
</script>