package com.sumridge.smart.bean;

public enum Color {
	Aqua("text-aqua"),
	Blue("text-blue"),
	Lightblue("text-light-blue"),
	Teal("text-teal"),
	Yellow("text-yellow"),
	Orange("text-orange"),
	Green("text-green"),
	Lime("text-lime"),
	Red("text-red"),
	Purple("text-purple"),
	Fuchsia("text-fuchsia"),
	Muted("text-muted"),
	Navy("text-navy");
	
	private String colorText;
	
	public String getColor(){
		return colorText;
	}
	
	private Color(String colorText){
		this.colorText = colorText;
	}

}
