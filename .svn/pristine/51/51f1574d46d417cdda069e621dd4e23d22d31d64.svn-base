<style type="text/css">
textarea[ui-mention], textarea[ui-mention] + * {
  line-height: 1em;
  font-size: 1em;
  padding: 5px;
  border: 1px;
  font-family: helvetica;
  font-weight: normal; }

textarea[ui-mention] {
  min-height: 100px;
  width: 100%;
  display: block;
  border: 1px solid #ccc;
  z-index: 2;
  position: relative; }

  textarea[ui-mention][ui-mention] {
    background: transparent; }

mention-container a {
  color: blue;
  text-decoration: underline;
  cursor: pointer; }
  a:hover {
    color: green; }

.mention-container {
  position: relative; }

.mention-highlight {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border: 1px;
  padding: 2px;
  z-index: 1;
  color: transparent;
  white-space: pre-wrap;
  border: 1px solid transparent;
  width: 100%; }

.mention-container .dropdown {
  position: absolute;
  top: 100%;
  min-width: 150px;
  max-width: 220px;
  right: 0;
  background: white;
  list-style: none;
  padding: 0;
  margin: 0;
  z-index:2;
  border: 1px solid #cecece;
}

.dropdown li{
    padding: 5px;
}

.dropdown .active {
    background-color: #cecece;
    color: #ff9494;
}

.photoTip {
    position: absolute;
    max-width: 350px;
    height: 200px;
    border: 1px solid #fcfcfc;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    top: 155px;
    left: 0px;
    z-index: 100;
    background-color: white;
}

#photoTip:before{
    content: " ";
    position: absolute;
    top: -5px;
    left: 8%;
    margin-left: -5px;
    width: 0;
    height: 0;
    border-bottom: 5px solid gray;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
}

#photoTip:after{
    content: " ";
    position: absolute;
    top: -4px;
    left: 8%;
    margin-left:-4px;
    width: 0;
    height: 0;
    border-bottom: 4px solid white;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
}
.box-body img{
    margin-left: 5px;
    margin-top: 5px;
    width: 100px;
    height: 100px;
}
.box-body img:nth-child(3n+1){
    margin-left: 0px;
}
#note img{
    width: 200px;
    height: 200px;
    margin-left: 10px;
    margin-top: 10px;
}
#note img:nth-child(3n+1),.addPic:nth-child(3n+1){
    margin-left: 0px;
}
#note img:hover{
    cursor: pointer;
}
.addPic{
    width: 100px;
    height: 100px;
    padding: 35px;
    border: 3px dashed gray;
    color: gray;
    margin-left: 5px;
    margin-top: 5px;
    display: inline-block;
    vertical-align: top;
}
.addPic:hover{
    border: 3px dashed orange;
    color: orange;
}
</style>

